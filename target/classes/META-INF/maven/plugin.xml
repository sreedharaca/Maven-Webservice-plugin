<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>Maven-Webservice-plugin</name>
  <description>Please feel free to suggest improvements or help support this plugin effort.</description>
  <groupId>org.sree.mavenplugin</groupId>
  <artifactId>Maven-Webservice-plugin</artifactId>
  <version>1.0.0</version>
  <goalPrefix>Maven-Webservice</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>jwsc</goal>
      <description>Runs the JWSC compiler task for web service enabled code.</description>
      <requiresDependencyResolution>compile</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.sree.mojo.wsgen.WSGenMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>contextPath</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>The deployed context of the web service.</description>
        </parameter>
        <parameter>
          <name>debug</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>The flag to set when debugging the process</description>
        </parameter>
        <parameter>
          <name>dependencies</name>
          <type>java.util.List</type>
          <required>true</required>
          <editable>false</editable>
          <description>This is the set of dependencies that are defined as part of this project&apos;s
pom which are active for the scope. You should not need to
override this unless your pom file is incomplete.</description>
        </parameter>
        <parameter>
          <name>descriptor</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The web.xml descriptor to use if a new one should not be generated. The
path should be fully qualified.
&lt;p&gt;
If there is more than one descriptor, use a comma &apos;,&apos; separated list.
&lt;/p&gt;</description>
        </parameter>
        <parameter>
          <name>explode</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>The flag to set when wanting exploded output. Defaults to true.</description>
        </parameter>
        <parameter>
          <name>inputDir</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The directory to find the jwsc enabled files.</description>
        </parameter>
        <parameter>
          <name>jwscTaskType</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The task type JAXRPC JAXWS</description>
        </parameter>
        <parameter>
          <name>keepGenerated</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>The keep generated flag for the mojo</description>
        </parameter>
        <parameter>
          <name>localRepository</name>
          <type>org.apache.maven.artifact.repository.ArtifactRepository</type>
          <required>false</required>
          <editable>true</editable>
          <description>The local repository</description>
        </parameter>
        <parameter>
          <name>mvnp</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description>This is the set of dependencies that are defined as part of this project&apos;s
pom which are active for the scope. You should not need to
override this unless your pom file is incomplete.</description>
        </parameter>
        <parameter>
          <name>optimize</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>The flag to set when requiring optimization. Defaults to true.</description>
        </parameter>
        <parameter>
          <name>outputDir</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The directory to output the generated code to.</description>
        </parameter>
        <parameter>
          <name>outputDirectory</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>This is the output directory for the artifacts. It defaults to
$project.build.directory.</description>
        </parameter>
        <parameter>
          <name>outputName</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>The name of the war to use when evaluating the ear file.</description>
        </parameter>
        <parameter>
          <name>pluginArtifacts</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description>These are the plugin artifacts for the weblogic mojo</description>
        </parameter>
        <parameter>
          <name>remoteRepositories</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description>The list of remote repositories</description>
        </parameter>
        <parameter>
          <name>sourceExcludes</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The explicit includes list for the file set</description>
        </parameter>
        <parameter>
          <name>sourceIncludes</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The explicit includes list for the file set</description>
        </parameter>
        <parameter>
          <name>toolsJar</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The the location of tools.jar file. this file must be dynamically added
to the classloader due to some issues with the way maven works and a bug
in jdk 1.6&lt;br/&gt;
&lt;i&gt;Note that the java.home location should be to the jre not the jdk&lt;/i&gt;</description>
        </parameter>
        <parameter>
          <name>verbose</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>The flag to set when desiring verbose output</description>
        </parameter>
      </parameters>
      <configuration>
        <debug implementation="boolean" default-value="false"/>
        <dependencies implementation="java.util.List">${project.dependencies}</dependencies>
        <explode implementation="boolean" default-value="true"/>
        <inputDir implementation="java.lang.String" default-value="${basedir}/src/main/java"/>
        <jwscTaskType implementation="java.lang.String" default-value="JAXRPC">${jwscTaskType}</jwscTaskType>
        <keepGenerated implementation="boolean" default-value="false">${weblogic.jwsc.keepGenerated}</keepGenerated>
        <localRepository implementation="org.apache.maven.artifact.repository.ArtifactRepository">${localRepository}</localRepository>
        <mvnp implementation="org.apache.maven.project.MavenProject">${project}</mvnp>
        <optimize implementation="boolean" default-value="true"/>
        <outputDir implementation="java.lang.String" default-value="${project.build.directory}"/>
        <outputDirectory implementation="java.lang.String">${project.build.directory}</outputDirectory>
        <outputName implementation="java.lang.String" default-value="${project.artifactId}-${project.version}"/>
        <pluginArtifacts implementation="java.util.List">${plugin.artifacts}</pluginArtifacts>
        <remoteRepositories implementation="java.util.List">${project.remoteArtifactRepositories}</remoteRepositories>
        <sourceExcludes implementation="java.lang.String">${weblogic.jwsc.excludes}</sourceExcludes>
        <sourceIncludes implementation="java.lang.String" default-value="**/*.java">${weblogic.jwsc.includes</sourceIncludes>
        <toolsJar implementation="java.lang.String">${java.home}/../lib/tools.jar</toolsJar>
        <verbose implementation="boolean" default-value="false"/>
      </configuration>
      <requirements>
        <requirement>
          <role>org.apache.maven.artifact.factory.ArtifactFactory</role>
          <field-name>artifactFactory</field-name>
        </requirement>
        <requirement>
          <role>org.apache.maven.artifact.resolver.ArtifactResolver</role>
          <field-name>resolver</field-name>
        </requirement>
      </requirements>
    </mojo>
